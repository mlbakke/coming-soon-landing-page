{
  "name": "coming-soon-landing-page",
  "version": "1.0.0",
  "description": "Landing page for a online store soon to launch",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/reset.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlbakke/coming-soon-landing-page.git"
  },
  "author": "Martin Liland Bakke",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mlbakke/coming-soon-landing-page/issues"
  },
  "homepage": "https://github.com/mlbakke/coming-soon-landing-page#readme",
  "devDependencies": {
    "autoprefixer": "^9.8.5",
    "concat": "^1.0.3",
    "node-sass": "^7.0.0",
    "postcss-cli": "^7.1.1"
  }
}
